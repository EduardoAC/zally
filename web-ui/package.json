{
  "name": "zally-web-ui",
  "version": "0.1.0",
  "description": "Zally API Linter Web UI",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node src/server/index.js",
    "dev": "NODE_ENV=development nodemon src/server/index.js",
    "build": "webpack -p",
    "docker:build": "./scripts/scm-source.js && docker build -t pierone.stups.zalan.do/architecture/zally-web-ui:${npm_package_version}-$(date +%s) .",
    "lint": "eslint .",
    "lint:checkstyle-report": "npm run lint -- -f checkstyle --output-file target/checkstyle.xml",
    "test": "npm run test:unit",
    "test:unit": "gulp test:unit",
    "test:unit:junit-report": "NODE_PATH=$NODE_PATH:./ gulp test:unit:junit-report",
    "test:unit:watch": "gulp test:unit:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zalando-incubator/zally.git"
  },
  "keywords": [
    "API",
    "Linter",
    "Zally"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zalando-incubator/zally/issues"
  },
  "homepage": "https://github.com/zalando-incubator/zally#readme",
  "dependencies": {
    "dotenv": "^4.0.0",
    "dotenv-parse-variables": "0.0.1",
    "express": "^4.15.2",
    "jquery": "^3.1.1",
    "oauth2-client-js": "0.0.15",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "request": "^2.81.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "chai": "^3.5.0",
    "eslint": "^3.17.1",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-istanbul": "^1.1.1",
    "gulp-mocha": "^4.1.0",
    "gulp-spawn-mocha": "^3.1.0",
    "node-scm-source": "^0.2.0",
    "nodemon": "^1.11.0",
    "proxyquire": "^1.7.11",
    "sinon": "^2.0.0",
    "sinon-chai": "^2.8.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  }
}
